{
  "packages": [
    "packages/*"
  ],
  "version": "independent",
  "command": {
    "version": {
      "conventionalCommits": true,
      "message": "chore(release): publish release [skip ci]",
      "concurrency": 8,
      "ignoreChanges": []
    },
    "publish": {
      "allowBranch": "main",
      "yes": true
    }
  }  
}
